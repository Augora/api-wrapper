{
  "name": "lbp-wrapper",
  "version": "0.8.3",
  "description": "La bete politique utilities for API calling :)",
  "author": "Kevin BACAS",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "build": "tsc",
    "prepublish": "npm run build",
    "lint": "tslint --project .",
    "test": "npm run lint && jest",
    "gen-docs": "typedoc --out docs ./src/index.ts",
    "deploy-docs": "npm run gen-docs && now alias --token=$NOW_TOKEN $(now ./docs --no-clipboard --token=$NOW_TOKEN --public) wrapper-docs.augora.fr"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.15",
    "now": "^16.1.1",
    "typedoc": "^0.15.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/lodash": "^4.14.138",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^3.5.3"
  }
}
