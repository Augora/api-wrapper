{
  "name": "lbp-wrapper",
  "version": "0.3.1",
  "description": "La bete politique utilities for API calling :)",
  "main": "dist/index.js",
  "author": "Kevin BACAS",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "lint": "tslint --project tslint.json",
    "build": "npm run clean && webpack --config webpack.config.js -p",
    "test": "npm run lint && jest",
    "start": "webpack-dev-server --config webpack.config.dev.js --open --hot",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@types/normalizr": "^2.0.18",
    "axios": "^0.16.2",
    "normalizr": "^3.2.3",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^4.0.0",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^20.0.4",
    "regenerator-runtime": "^0.10.5",
    "source-map-loader": "^0.2.1",
    "ts-jest": "^20.0.7",
    "tslint": "^5.5.0",
    "tslint-config-airbnb": "^5.0.1",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "transform": {
      ".(ts|tsx|js)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
