{
  "name": "lbp-wrapper",
  "version": "0.8.2",
  "description": "La bete politique utilities for API calling :)",
  "author": "Kevin BACAS",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "build": "tsc",
    "prepublish": "npm run build",
    "lint": "tslint --project .",
    "test": "npm run lint && jest",
    "doc": "typedoc --out docs ./src/index.ts",
    "deploy-docs": "now alias --token=$NOW_TOKEN $(now ./docs --no-clipboard --token=$NOW_TOKEN --public) wrapper-docs.augora.fr"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.10",
    "typedoc": "^0.14.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.135",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^3.4.5"
  }
}
